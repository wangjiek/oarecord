    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        TYPEID AS typeid, DAOGUANID AS daoguanid, JZID AS jzid, KSID AS ksid, ZYZGID AS zyzgid, YQID AS yqid, CREATETIME AS createtime, USERID AS userid, TYPENAME AS typename
    </sql>

</mapper>
